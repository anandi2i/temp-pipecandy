{
  "name": "campaignAudit",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "email": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "string",
      "required": true,
      "postgresql": {
        "dataType": "text"
      }
    },
    "mailId": {
      "type": "String"
    },
    "threadId": {
      "type": "String"
    },
    "subject": {
      "type": "string"
    },
    "sentTime": {
      "type": "date"
    },
    "fromEmail": {
      "type": "string",
      "required": true
    },
    "isEligibleToFollowUp": {
      "type": "boolean",
      "default": true
    },
    "createdAt": {
      "type": "date",
      "defaultFn": "now"
    },
    "updatedAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "campaign": {
      "type": "belongsTo",
      "model": "campaign",
      "foreignKey": "campaignId"
    },
    "person": {
      "type": "belongsTo",
      "model": "person",
      "foreignKey": "personId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "inboxMails"
    }
  ],
  "methods": {}
}
